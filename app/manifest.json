{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/storage.js",
      "scripts/request.js",
      "scripts/dollar_l.js",
      "scripts/icon.js",
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "background",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "browser_action": {
    "default_title": "Parameterizer"
  }
}
